<script>
   function cancelOrder(proId,ordId) {
    console.log( proId,ordId);
    $.ajax({
      url: "/cancel-order",
      data: {
        proId:proId,
        orderId: ordId,
      },
      method: "post",
      success: (response) => {
        console.log("HELLOOO");
        if(response){
          location.reload()
        }
        
      },
    });
    Swal.fire('Canceled')
  }
</script>